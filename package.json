{
  "name": "react-typer-component",
  "version": "0.0.0",
  "description": "A react component lets you create typewriter effect.",
  "keywords": [
    "react",
    "react component",
    "typewriter"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/react-typer-component.umd.js",
  "module": "./dist/react-typer-component.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-typer-component.es.js",
      "require": "./dist/react-typer-component.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:example": "tsc && vite build",
    "build:lib": "tsc --project tsconfig.lib.json && vite build --mode lib",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^1.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9"
  }
}
